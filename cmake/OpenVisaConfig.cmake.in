@PACKAGE_INIT@

if(${CMAKE_SIZEOF_VOID_P} EQUAL 8)
 set(_PLATFORM x64)
else()
 set(_PLATFORM x86)
endif()

set(@TARGET_NAME@_ROOT "@CMAKE_INSTALL_PREFIX@")
set(@TARGET_NAME@_INCLUDEDIR "${@TARGET_NAME@_ROOT}/include")
set(@TARGET_NAME@_LIBRARYDIR "${@TARGET_NAME@_ROOT}/lib/${_PLATFORM}")
set(@TARGET_NAME@_BINDIR "${@TARGET_NAME@_ROOT}/bin/${_PLATFORM}/${CMAKE_BUILD_TYPE}")
check_required_components(@TARGET_NAME@)
unset(_PLATFORM)

include_directories(${@TARGET_NAME@_INCLUDEDIR})
if (NOT TARGET @TARGET_NAME@)
 add_library(@TARGET_NAME@ SHARED IMPORTED)
 # set_target_properties(@TARGET_NAME@ PROPERTIES
 # IMPORTED_LOCATION_RELEASE ${@TARGET_NAME@_LIBRARYDIR}/@TARGET_NAME@.lib
 # IMPORTED_LOCATION_DEBUG ${@TARGET_NAME@_LIBRARYDIR}/@TARGET_NAME@d.lib)

 if (MSVC)
  if(${CMAKE_BUILD_TYPE} EQUAL "Debug")
   set_property(TARGET @TARGET_NAME@ PROPERTY IMPORTED_LOCATION ${@TARGET_NAME@_BINDIR}/@TARGET_NAME@.dll)
   set_property(TARGET @TARGET_NAME@ PROPERTY IMPORTED_IMPLIB ${@TARGET_NAME@_LIBRARYDIR}/@TARGET_NAME@.lib)
  else()
   set_property(TARGET @TARGET_NAME@ PROPERTY IMPORTED_LOCATION ${@TARGET_NAME@_BINDIR}/@TARGET_NAME@d.dll)
   set_property(TARGET @TARGET_NAME@ PROPERTY IMPORTED_IMPLIB ${@TARGET_NAME@_LIBRARYDIR}/@TARGET_NAME@d.lib)
  endif(${CMAKE_BUILD_TYPE} EQUAL "Debug")
 endif(MSVC)
endif(NOT TARGET @TARGET_NAME@)

